<%- include('partials/header'); -%>
<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input type="text" name="titlePost" class="form-control" />
    <label>Post</label>
    <textarea
      name="contentPost"
      class="form-control"
      rows="5"
      cols="30"
    ></textarea>
  </div>
  <button type="submit" class="btn-primary btn">Publish</button>
</form>
<%- include('partials/footer'); -%>
